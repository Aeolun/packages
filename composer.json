{
  "name": "terramar/packages",
  "description": "Terramar Labs Repository. Adds additional functionality on top of Composer Satis",
  "license": "MIT",
  "require": {
    "php": ">=5.4.0",
    "composer/satis": "dev-master",
    "composer/composer": "1.0.x-dev",
    "doctrine/cache": "~1.3",
    "twig/twig": "~1.15",
    "symfony/debug": "~2.4",
    "symfony/console": "~2.4",
    "symfony/process": "~2.4",
    "symfony/yaml": "~2.4",
    "m4tthumphrey/php-gitlab-api": "dev-master",
    "nikic/fast-route": "dev-master",
    "tyler-sommer/nice": "1.0.x-dev"
  },
  "require-dev": {
    "phpunit/phpunit": "~3.6"
  },
  "autoload": {
    "psr-0": {
      "Terramar\\Packages": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "2.0-dev"
    }
  },
  "minimum-stability": "beta",
  "prefer-stable": true
}
